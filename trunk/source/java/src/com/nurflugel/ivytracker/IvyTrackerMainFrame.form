<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.nurflugel.ivytracker.IvyTrackerMainFrame">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="595" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="8a66c" binding="buttonPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="3fec4" layout-manager="GridBagLayout">
            <constraints border-constraint="West"/>
            <properties/>
            <border type="etched" title="Ivy usage output"/>
            <children>
              <component id="460d" class="javax.swing.JRadioButton" binding="showAllLibrariesRadioButton" default-binding="true">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="1.0"/>
                </constraints>
                <properties>
                  <selected value="true"/>
                  <text value="Show all libraries"/>
                </properties>
              </component>
              <component id="83742" class="javax.swing.JRadioButton" binding="showOnlyUsedLibrariesRadioButton" default-binding="true">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="1.0"/>
                </constraints>
                <properties>
                  <text value="Show only used libraries"/>
                </properties>
              </component>
              <component id="6941" class="javax.swing.JRadioButton" binding="showOnlyUnusedLibrariesRadioButton" default-binding="true">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="1.0"/>
                </constraints>
                <properties>
                  <text value="Show only unused libraries"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="8f6d9" layout-manager="GridBagLayout">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="etched" title="Filter text"/>
            <children>
              <component id="18be5" class="javax.swing.JTextField" binding="filterField">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="150" height="-1"/>
                  </grid>
                  <gridbag weightx="1.0" weighty="1.0"/>
                </constraints>
                <properties/>
              </component>
            </children>
          </grid>
          <grid id="7c582" layout-manager="GridBagLayout">
            <constraints border-constraint="East"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="a39a0" class="javax.swing.JButton" binding="quitButton" default-binding="true">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="0.0"/>
                </constraints>
                <properties>
                  <text value="Quit"/>
                </properties>
              </component>
              <component id="d2500" class="javax.swing.JButton" binding="showProjectsTreeButton" default-binding="true">
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="0.0"/>
                </constraints>
                <properties>
                  <enabled value="true"/>
                  <text value="Show projects tree"/>
                  <toolTipText value="Show a tree of projects so you can drill down"/>
                </properties>
              </component>
              <component id="de5e1" class="javax.swing.JButton" binding="exportTableToClipboardButton" default-binding="true">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="1.0" weighty="0.0"/>
                </constraints>
                <properties>
                  <enabled value="true"/>
                  <text value="Export table to clipboard"/>
                  <toolTipText value="Export the visible table rows to your cut and paste clipboard"/>
                </properties>
              </component>
              <component id="983d4" class="javax.swing.JButton" binding="findMissingIvyFilesButton" default-binding="true">
                <constraints>
                  <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="0.0" weighty="0.0"/>
                </constraints>
                <properties>
                  <text value="Find missing Ivy files"/>
                  <toolTipText value="Find Ivy files referred to by other Ivy files, but aren't in the repository"/>
                </properties>
              </component>
              <component id="a7ed4" class="javax.swing.JButton" binding="specifyIvyReposioryButton" default-binding="true">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                  <gridbag weightx="0.0" weighty="0.0"/>
                </constraints>
                <properties>
                  <text value="Specify Ivy Reposiory"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="cd15" binding="statusPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="7ce65" class="javax.swing.JLabel" binding="statusLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <horizontalAlignment value="0"/>
              <text value="Please wait while the repository is scanned..."/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="d8465" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <scrollpane id="dd2e9">
            <constraints border-constraint="Center"/>
            <properties>
              <toolTipText value="Click on a row to see what projects use this item"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="e1506" class="javax.swing.JTable" binding="resultsTable">
                <constraints/>
                <properties/>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
    </children>
  </grid>
  <buttonGroups>
    <group name="ivyUsageButtonGroup">
      <member id="460d"/>
      <member id="83742"/>
      <member id="6941"/>
    </group>
  </buttonGroups>
</form>
